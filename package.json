{
  "name": "nexus-prisma-crud",
  "version": "0.0.0",
  "description": "Nexus plugin to create quickly a CRUD API using Prisma as client.",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "nexus-prisma-crud": "./dist/cli/nexus-prisma-crud.js"
  },
  "scripts": {
    "dev": "yarn build --watch",
    "build": "rimraf ./dist && tsc -d",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write '**/*'",
    "prepublishOnly": "yarn build"
  },
  "license": "MIT",
  "devDependencies": {
    "@prisma/sdk": "^3.0.2",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "fs-jetpack": "^4.1.1",
    "graphql": "^15.5.1",
    "jest": "^26.6.3",
    "nexus": "^1.1.0",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^2.1.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "typescript": "^4.4.2"
  },
  "peerDependencies": {
    "graphql": "*",
    "nexus": "^1.1.0"
  },
  "dependencies": {
    "@prisma/generator-helper": "^3.0.2",
    "ts-morph": "^12.0.0"
  },
  "homepage": "https://github.com/mozartspa/nexus-prisma-crud",
  "repository": {
    "type": "git",
    "url": "https://github.com/mozartspa/nexus-prisma-crud.git"
  },
  "bugs": {
    "url": "https://github.com/mozartspa/nexus-prisma-crud/issues"
  },
  "keywords": [
    "nexus",
    "graphql",
    "crud",
    "prisma",
    "generator"
  ]
}
